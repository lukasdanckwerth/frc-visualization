{
  "name": "frc-visualisation",
  "version": "1.0.50",
  "description": "French Rap Corpus Visualization",
  "main": "npm run serve",
  "author": "Lukas Danckwerth",
  "license": "MIT",
  "scripts": {
    "build": "rollup --config",
    "build:watch": "rollup --config --watch",
    "serve": "http-server public/",
    "serve:production": "NODE_ENV=production node server.js production",
    "assets": "npm run build && node utils/generate.assets.js",
    "assets:search": "node utils/datasets.search.innovation.list.js"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "d3": "^6.5.0",
    "rollup": "^2.39.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-stream": "^1.24.1"
  },
  "dependencies": {
    "html2canvas": "^1.4.0",
    "http-server": "^14.0.0",
    "puppeteer": "^13.0.1"
  }
}